  3.9410351810828457`*^9}},ExpressionUUID->"28bccd2f-7cf3-4e4f-b4e5-\
641f549b57d3"],

Cell[BoxData[
 RowBox[{
  RowBox[{"M", "=", 
   RowBox[{"ReleaseHold", "[", 
    RowBox[{
     RowBox[{
      RowBox[{
       FractionBox[
        RowBox[{"-", "1"}], 
        RowBox[{"16", 
         SuperscriptBox["\[Pi]", "2"]}]], 
       RowBox[{"(", 
        RowBox[{"LoopIntegrate", "[", 
         RowBox[{
          RowBox[{"\[LeftAngleBracket]", 
           RowBox[{
            RowBox[{"\[ScriptU]", "[", 
             RowBox[{"q2", ",", "mi"}], "]"}], ",", 
            RowBox[{"I", "*", "VertexJI"}], ",", 
            RowBox[{"I", "*", 
             RowBox[{"(", 
              RowBox[{
               RowBox[{
                RowBox[{"(", 
                 RowBox[{"k", "+", "q2", "-", "q1"}], ")"}], ".", 
                "\[Gamma]"}], "+", 
               RowBox[{"mj", " ", "\[DoubleStruckOne]"}]}], ")"}]}], ",", 
            RowBox[{
             RowBox[{"-", "I"}], "*", "e", "*", 
             SubscriptBox["\[Gamma]", "\[Mu]"]}], ",", 
            RowBox[{"I", "*", 
             RowBox[{"(", 
              RowBox[{
               RowBox[{"k", ".", "\[Gamma]"}], "+", 
               RowBox[{"mj", " ", "\[DoubleStruckOne]"}]}], ")"}]}], ",", 
            RowBox[{"I", "*", "VertexIJ"}], ",", 
            RowBox[{"\[ScriptU]", "[", 
             RowBox[{"q1", ",", "mi"}], "]"}]}], "\[RightAngleBracket]"}], 
          ",", "k", ",", 
          RowBox[{"{", 
           RowBox[{
            RowBox[{
             RowBox[{"-", "q1"}], "+", "k"}], ",", "m\[Phi]"}], "}"}], ",", 
          RowBox[{"{", 
           RowBox[{
            RowBox[{"q2", "-", "q1", "+", "k"}], ",", "mj"}], "}"}], ",", 
          RowBox[{"{", 
           RowBox[{"k", ",", "mj"}], "}"}]}], "]"}], ")"}]}], "/.", 
      "kinematics"}], "//", "LoopRefine"}], "]"}]}], ";"}]], "Input",
 CellChangeTimes->{{3.941029563716116*^9, 3.941029573187847*^9}, {
  3.941035282337665*^9, 3.941035314139318*^9}, {3.9410379848423357`*^9, 
  3.941037987491693*^9}, {3.9410382270706882`*^9, 3.94103822823569*^9}},
