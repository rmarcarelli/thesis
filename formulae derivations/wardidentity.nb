     SuperscriptBox["\[ExponentialE]", 
      RowBox[{"2", " ", "\[ImaginaryI]", " ", "\[Delta]"}]]}], ")"}], " ", 
   SuperscriptBox["g", "2"], " ", "mj", " ", 
   RowBox[{"(", 
    RowBox[{
     RowBox[{"2", " ", 
      SuperscriptBox["mi", "2"], " ", 
      RowBox[{"DiscB", "[", 
       RowBox[{
        SuperscriptBox["mi", "2"], ",", "mj", ",", "m\[Phi]"}], "]"}]}], "-", 
     
     RowBox[{"2", " ", 
      SuperscriptBox["mi", "2"], " ", 
      RowBox[{"DiscB", "[", 
       RowBox[{
        RowBox[{"q", ".", "q"}], ",", "mj", ",", "mj"}], "]"}]}], "-", 
     RowBox[{
      RowBox[{"(", 
